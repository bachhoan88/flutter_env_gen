# Staging Environment Configuration
API_BASE_URL=https://staging-api.example.com
ENABLE_LOGS=true
CACHE_TIMEOUT=7200
MAX_RETRY_ATTEMPTS=5
SUPPORTED_LOCALES=en,vi,zh,ja,ko
APP_NAME=MyApp STAGING

# Database Configuration
DB_HOST=staging.database.com
DB_PORT=5432
DB_NAME=myapp_staging

# Feature Flags
FEATURE_DARK_MODE=true
FEATURE_PUSH_NOTIFICATIONS=true

# Sensitive data (will be obfuscated)
API_KEY=staging_secret_key_67890
JWT_SECRET=staging_jwt_secret_token